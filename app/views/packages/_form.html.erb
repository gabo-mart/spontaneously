
<%= form_for([@vendor, @package]) do |f| %>


  <div class="field">
    <%= f.label :itinerary %><br>
    <%= f.text_field :itinerary %>
  </div>
  <!-- <div class="field">
    <%= f.label :ship %><br>
    <%= f.text_field :ship %>
  </div> -->
  <div class="field">
    <%= f.label :start_date %><br>
    <%= f.date_field :start_date %>
  </div>
  <div class="field">
    <%= f.label :end_date %><br>
    <%= f.date_field :end_date %>
  </div>
  <div class="field">
    <%= f.label :duration %><br>
    <%= f.text_field :duration %>
  </div>
  <div class="field">
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </div>
  <div class="field">
    <%= f.label :rooms %><br>
    <%= f.number_field :rooms %>
  </div>
  <div class="field">
    <%= f.label :people %><br>
    <%= f.number_field :people %>
  </div>
  <div class="field">
    <%= f.label :room_type %><br>
    <%= f.text_field :room_type %>
  </div>

  <div class="input-field col s6">
    <%= select("Ship", "ship_id", Ship.all.collect {|s| [ s.name, s.id ] }) %>
    <label>Ship </label>
  </div>

  <div class="field">
    <%= f.file_field :avatar %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
