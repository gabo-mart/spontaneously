<class id="body">
	<div class="container">
		<div class="logincompany">
			<h1>Company Log In</h1>

			<%= form_tag create_vendor_path do %>
			<p class="loginform">
				<%= label_tag :email %>
				<%= text_field_tag :email %>
			</p>
			<p class="loginform">
				<%= label_tag :password %>
				<%= password_field_tag :password %>
			</p>

			<%= submit_tag 'Log In', :class => "btn btn-success" %>

			<% end %>
		</div>
	</div>
