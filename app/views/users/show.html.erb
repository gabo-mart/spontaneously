<div class="pagecontainer">
<table id="packageTable" class="highlight centered">
  <thead></thead>
  <tbody>
    <% @vendor.ships.each do |ship| %>
      <tr onclick='window.location="<%= vendor_ship_path(@vendor.id, ship.id) %>"' >
<p id="notice"><%= notice %></p>
<table id="packageTable" class="highlight centered">
  <thead>
    <tr>
      <th>Package</th>
      <th>Duration</th>
      <th>Dates</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.packages.each do |package| %>
      <tr>
        <td><%= package.itinerary %></td>
        <td><%= package.duration %></td>
        <td><%= package.start_date.to_formatted_s(:long)  %> <%= + " - " + package.end_date.to_formatted_s(:long)  %></td>
        <td><%= package.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Dob:</strong>
  <%= @user.dob %>
</p>

<p>
  <strong>Country:</strong>
  <%= @user.country %>
</p>

<p>
  <strong>Address1:</strong>
  <%= @user.address1 %>
</p>

<p>
  <strong>Address2:</strong>
  <%= @user.address2 %>
</p>

<p>
  <strong>Region:</strong>
  <%= @user.region %>
</p>

<p>
  <strong>Postal code:</strong>
  <%= @user.postal_code %>
</p>

<p>
  <strong>Tel:</strong>
  <%= @user.tel %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>


<%= link_to 'Edit', edit_user_path(@user) %>
</div>
