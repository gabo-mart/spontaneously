
<%= form_for([@vendor, @package]) do |f| %>


  <div class="field">
    <%= f.label :itinerary %><br>
    <%= f.text_field :itinerary %>
  </div>
  <!-- <div class="field">
    <%= f.label :ship %><br>
    <%= f.text_field :ship %>
  </div> -->
  <div class="field">
    <%= f.label :dates %><br>
    <%= f.text_field :dates %>
  </div>
  <div class="field">
    <%= f.label :duration %><br>
    <%= f.text_field :duration %>
  </div>
  <div class="field">
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </div>
  <div class="field">
    <%= f.label :rooms %><br>
    <%= f.number_field :rooms %>
  </div>
  <div class="field">
    <%= f.label :people %><br>
    <%= f.number_field :people %>
  </div>
  <div class="field">
    <%= f.label :room_type %><br>
    <%= f.text_field :room_type %>
  </div>
  <div class="field">
    <div><strong>Ship</strong></div>
    <%= f.select :ship_id, options_for_select(Ship.all.collect{ |u| [u.name, u.id] }) %>
  </div>
  <div class="field">
    <%= f.file_field :avatar %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
