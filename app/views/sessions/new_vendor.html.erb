<class id="body">
	<div class="pagecontainer">
		<div id="logincompany">
			<h1>Company Log In</h1>

			<%= form_for create_vendor_path do |f| %>
			<p>
				<%= f.label :email %>
				<%= f.text_field :email, :class => "form-control"  %>
			</p>
			<p>
				<%= f.label :password %>
				<%= f.password_field :password, :class => "form-control" %>
			</p>

			<%= f.submit 'Log In', :class => "btn btn-success" %>

			<% end %>
		</div>
	</div>
