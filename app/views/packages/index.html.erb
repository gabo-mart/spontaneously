<h1>Listing Packages</h1>

<table>
  <thead>
    <tr>
      <th>Itinerary</th>
      <th>Ship</th>
      <th>Dates</th>
      <th>Duration</th>
      <th>Price</th>
      <th>Rooms</th>
      <th>People</th>
      <th>Room type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.itinerary %></td>
        <td><%= package.ship.name %></td>
        <td><%= package.start_date.to_formatted_s(:long)  %> <%= + " - " + package.end_date.to_formatted_s(:long)  %></td>
        <td><%= package.duration %></td>
        <td><%= package.price %></td>
        <td><%= package.rooms %></td>
        <td><%= package.people %></td>
        <td><%= package.room_type %></td>
        <td><%= link_to 'Show', vendor_package_path(package.vendor, package.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
