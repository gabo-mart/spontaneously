<p id="notice"><%= notice %></p>

<p>
  <strong>Company:</strong>
  <%= @vendor.company %>
</p>
<div> SHIPS </div>
<table>
	<thead></thead>
	<tbody>
		<% @vendor.ships.each do |ship| %>
			<tr>
				<td><%= link_to ship.name, vendor_ship_path(@vendor,ship) %></td>
			</tr>
		<% end %>
	</tbody>
</table>



<% if @vendor == current_vendor %>
<%= link_to 'Create a New Ship', new_vendor_ship_path(@vendor) %> |
<%= link_to 'Edit Vendor Profile', edit_vendor_path(@vendor) %> 
<% end %>
